count=1
echo "[" >> $3.json
while [ $count -le $2 ]; do
    ./explorer \
    --rule `./randrule --randseed $RANDOM` \
    --no-write-end-state \
    --init-topo tree \
    --init-tape single-end \
    --nodes $1 --iterations $1 --cycle-check-depth $1 >> $3.json
    ((count++))
    if [ $count -le $2 ]; then
        echo "," >> $3.json
    fi
done
echo "]" >> $3.json
