./randrule --reset --cache "$3.cache"
iteration=1
while [ $iteration -le $2 ]; do
    ./explorer \
    --rule `./randrule --randseed $RANDOM --reserve $2 --cache "$3.cache"` \
    --no-write-end-state \
    --nodes $1 --iterations $1 --cycle-check-depth $1 >> $3.json
    echo "" >> $3.json
    ((iteration++))
done
