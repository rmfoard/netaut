runnr=1
while [ $runnr -le $2 ]; do
    ../code/explorer \
    --rule `../code/rulesource --cache "$8"` \
    --randseed $RANDOM \
    --no-write-end-state \
    --nodes $1 \
    --max-iterations $3 \
    --init-tape $4 \
    --init-topo $5 \
    --cycle-check-depth $6 \
    $7 >> $8.json
    echo "" >> $8.json
    ((runnr++))
done
